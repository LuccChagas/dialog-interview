{
    "swagger": "2.0",
    "info": {
        "description": "API Documentation",
        "title": "Dialog-Interview Swagger API",
        "contact": {
            "name": "Luccas Machado",
            "url": "linkedin",
            "email": "luccaa.chagas23@gmail.com"
        },
        "version": "1.0"
    },
    "basePath": "/v1",
    "paths": {
        "/authors/:name": {
            "get": {
                "description": "get []models.Authors",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Authors"
                ],
                "summary": "Get authors by Name",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Author Name",
                        "name": "name",
                        "in": "path"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/models.Authors"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/errs.Handling"
                        }
                    }
                }
            }
        },
        "/authors/:page": {
            "get": {
                "description": "get models.AuthorsResultSet",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Authors"
                ],
                "summary": "Show all authors or filter by Page",
                "parameters": [
                    {
                        "type": "integer",
                        "description": "Pagination item",
                        "name": "page",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/models.AuthorsResultSet"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/errs.Handling"
                        }
                    }
                }
            }
        },
        "/books/create": {
            "post": {
                "description": "POST models.Book",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Books"
                ],
                "summary": "Create new Books",
                "parameters": [
                    {
                        "description": "Book",
                        "name": "Book",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/models.Book"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/errs.Handling"
                        }
                    }
                }
            }
        },
        "/books/read": {
            "get": {
                "description": "GET models.Book",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Books"
                ],
                "summary": "Get Books",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Book Name",
                        "name": "name",
                        "in": "path"
                    },
                    {
                        "type": "string",
                        "description": "Book Edition",
                        "name": "edition",
                        "in": "path"
                    },
                    {
                        "type": "string",
                        "description": "Book Publication_Year",
                        "name": "publication_year",
                        "in": "path"
                    },
                    {
                        "type": "string",
                        "description": "Book Author ID",
                        "name": "author",
                        "in": "path"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/errs.Handling"
                        }
                    }
                }
            }
        },
        "/books/update": {
            "put": {
                "description": "PUT models.Book",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Books"
                ],
                "summary": "Update Books",
                "parameters": [
                    {
                        "description": "Book",
                        "name": "Book",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/models.Book"
                        }
                    }
                ],
                "responses": {
                    "204": {
                        "description": "No Content"
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/errs.Handling"
                        }
                    }
                }
            },
            "delete": {
                "description": "DELETE models.Book",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Books"
                ],
                "summary": "Delete Books",
                "responses": {
                    "204": {
                        "description": "No Content"
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/errs.Handling"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "errs.Handling": {
            "type": "object",
            "properties": {
                "error": {
                    "type": "string"
                },
                "message": {
                    "type": "string"
                }
            }
        },
        "models.Authors": {
            "type": "object",
            "properties": {
                "id": {
                    "type": "integer"
                },
                "name": {
                    "type": "string"
                }
            }
        },
        "models.AuthorsResultSet": {
            "type": "object",
            "properties": {
                "authors": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/models.Authors"
                    }
                },
                "pagination": {
                    "$ref": "#/definitions/models.Pagination"
                }
            }
        },
        "models.Book": {
            "type": "object",
            "properties": {
                "authors": {
                    "type": "array",
                    "items": {
                        "type": "integer"
                    }
                },
                "edition": {
                    "type": "integer"
                },
                "id": {
                    "type": "integer"
                },
                "name": {
                    "type": "string"
                },
                "publication_year": {
                    "type": "integer"
                }
            }
        },
        "models.Pagination": {
            "type": "object",
            "properties": {
                "pages": {
                    "type": "integer"
                },
                "total": {
                    "type": "integer"
                }
            }
        }
    }
}